<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netcoreapp2.1</TargetFramework>
        <!-- NOTE: Version controlled via Directory.Build.props  -->
        <!--<Version></Version>-->
        <RootNamespace>com.github.sushantmimani</RootNamespace>
        <LangVersion>7.1</LangVersion>
        <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
        <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
        <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
        <!-- Properties related to NuGet packaging: -->
        <PackageId>com.github.sushantmimani.helloworld</PackageId>
        <Title>FirstHelloWorld</Title>
        <Description>Testing nuget deployments</Description>
        <Authors>Sushant Mimani</Authors>
        <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
        <PackageProjectUrl>https://github.com/sushantmimani/test-deploy</PackageProjectUrl>
        <RepositoryUrl>https://github.com/sushantmimani/test-deploy</RepositoryUrl>
        <IncludeSymbols>False</IncludeSymbols>
        <!-- End of Properties related to NuGet packaging: -->
    </PropertyGroup>

</Project>